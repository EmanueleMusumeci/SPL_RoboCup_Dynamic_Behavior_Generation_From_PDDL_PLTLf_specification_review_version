        </Body>
        <Body ref="Nao" name="robot3">
        <Translation x="1.4" y="-1.1" z="320mm"/>
        <Rotation z="90degree"/> 
        </Body>


            
    </Compound>

    <Compound name="extras">

        <Body ref="Nao" name="robot7">
        <Translation x="1.4" y="0" z="320mm"/>
        <Rotation z="90degree"/>
        <Set name="NaoColor" value="dblue"/>
        </Body>
        <Body ref="Nao" name="robot8">
        <Translation x="2" y="0" z="320mm"/>
        <Rotation z="90degree"/>
        <Set name="NaoColor" value="dblue"/>
        </Body>
        <Body ref="Nao" name="robot9">
        <Translation x="2.6" y="0" z="320mm"/>
        <Rotation z="90degree"/>
        <Set name="NaoColor" value="dblue"/>
        </Body>

    </Compound>

    <Compound name="balls">
      <Body ref="ball">
        <Translation x="2m" y="1.5" z="1m"/>
      </Body>
    </Compound>

    <Compound ref="field"/>

  </Scene>

</Simulation>
