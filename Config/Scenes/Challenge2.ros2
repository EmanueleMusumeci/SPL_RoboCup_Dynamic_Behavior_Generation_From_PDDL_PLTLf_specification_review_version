    <Compound name="teamColors">
      <Appearance name="black"/>
      <Appearance name="blue"/>
    </Compound>

    <Compound name="robots">
        <Body ref="Nao" name="robot2">
        <Translation x="2" y="1.7" z="320mm"/>
